%ul.none
  - if @applicant.descision_response['raw_responses']['Socure 30'][0]['kyc']['fieldValidations']['mobileNumber'] == 0.99
    %li
      = descision_icon('Approved')
      Socure 30
      = t('admin.applicants.show.matches.phone')
  - if @applicant.descision_response['raw_responses']['White Pages Pro'][0]['phone_checks']['is_valid']
    %li
      = descision_icon('Approved')
      WPP
      = t('admin.applicants.show.matches.phone')
  - if @applicant.descision_response['raw_responses']['Lexis Nexis Instant ID'][0]['response']['Result']['VerifiedInput']['HomePhone'].present?
    %li
      = descision_icon('Approved')
      LexisNexis
      = t('admin.applicants.show.matches.phone')
